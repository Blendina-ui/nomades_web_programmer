  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test Node installation",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test_nodejs.js", 
    }
  ]
}